{% extends "./nunjucks/layouts/layout.njk" %}


{% block pageBody %}
    <h1>It works !!</h1>

    {% include 'nunjucks/partials/partial-example.njk' %}
    {% include 'nunjucks/partials/partial-example-2.njk' %}

    <style>
        @keyframes dash {
            from {
                stroke-dashoffset: 2000;
            }
            to {
                stroke-dashoffset: 0;
            }
        }

        .svg-sprite {
            fill: transparent;
            stroke: #222;
            stroke-width: 0.8;
            stroke-linecap: round;
            stroke-linejoin: round;
            stroke-dasharray: 2000;
            stroke-dashoffset: 0;
            animation: dash 2s ease;
        }
    </style>

    {% svg 'alarm-clock-outlined-tool', 'abstract-shape-outline' %}
{% endblock %}
